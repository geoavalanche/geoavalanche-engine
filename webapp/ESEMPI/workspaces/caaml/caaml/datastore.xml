<dataStore>
  <id>caaml_datastore</id>
  <name>caaml</name>
  <type>PostGIS</type>
  <enabled>true</enabled>
  <workspace>
    <id>caaml_workspace</id>
  </workspace>
  <connectionParameters>
    <entry key="schema">${caaml.schema}</entry>
    <entry key="port">${caaml.port}</entry>
    <entry key="dbtype">${caaml.dbtype}</entry>
    <entry key="host">${caaml.host}</entry>
    <entry key="password">${caaml.password}</entry>
    <entry key="user">${caaml.user}</entry>
    <entry key="database">${caaml.database}</entry>
    <entry key="namespace">${caaml.host}</entry>
  </connectionParameters>
  <__default>false</__default>
</dataStore>